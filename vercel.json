{
  "version": 2,
  "builds": [
    {
      "src": "**/*.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/process-receipts",
      "dest": "app.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/generate-report",
      "dest": "app.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}